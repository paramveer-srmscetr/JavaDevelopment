abstract class  Animal{
    
    public abstract void Bark();
    
    
    public void Color(){
        System.out.println("Black color ");
    }
}

class Dog extends Animal{
    @Override
    public void Bark(){
        System.out.println("Dog barked");
    }
}


public class Main
{
	public static void main(String[] args) {
	
	 // abstraction--data binding 
	 
	Animal obj=new Dog();
	obj.Bark();
	obj.Color();
	
	}
}
